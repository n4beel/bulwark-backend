{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true
  },
  "watchAssets": {
    "ignore": [
      "temp/**/*",
      "dist/**/*",
      "node_modules/**/*"
    ]
  }
}